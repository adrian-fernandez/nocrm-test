<% unless @commits.blank? %>
	<h2>Commits</h2>
	<% @commits.each do |commit| %>
		<p>
			<span><strong>ID: </strong><a href="<%=commit['url']%>"><%=commit['tree_id']%></a></span>
			<span><strong>Message: </strong><%= commit['message'] %></span>
			<span><strong>Author: </strong><%= commit['author']['name'] %></span>
		</p>
	<% end %>
<% end %>

<br/>

<%= raw @content %>